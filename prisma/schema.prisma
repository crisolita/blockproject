generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int     @id @default(autoincrement())
  fullName   String
  balance    Int?
  email      String? @unique
  password   String?
  wallet     String?
  authToken  String?
  typeOfUser String
}

model keys {
  id     Int     @id @default(autoincrement())
  wallet String?
  key    String?
  second String?
}
