generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  fullName String
  balance  Int?
  email    String? @unique
  password String?
  wallet   String?
  verified Boolean?
}
